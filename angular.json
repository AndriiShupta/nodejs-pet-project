                {
                  "replace": "apps/nest-monolith/src/environments/environment.ts",
                  "with": "apps/nest-monolith/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "nest-monolith:build"
